{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": "vite",
    "outputDirectory": "build/client",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "ignoreCommand": "[ \"$VERCEL_ENV\" != production ]",
    "cleanUrls": true,
    "github": {
      "enabled": true,
      "autoJobCancelation": true
    }
  }